<chart Caption='Rating of ice cream flavors by brands' bgColor='ED73AC, F7BFC8' bgAngle='315' baseFontColor='333333' showValues='1'>
    
        <rows>
            <row id='Chocolate'/>
            <row id='Vanilla'/>
            <row id='Strawberry'/>
            <row id='Butter Pecan'/>
            <row id='Pistachio'/>
        </rows>

        <columns>
               <column id='Baskin-Robbins' />
               <column id='Blue Bunny' />
               <column id='Turkey Hill' />
               <column id='Carvel' />
               <column id='Breyers' />
        </columns>
        
    <dataset>
    
		<set rowId='Chocolate' columnId='Baskin-Robbins' showValue='0' value='3'/>
		<set rowId='Chocolate' columnId='Blue Bunny' showValue='0' value='4'/>
		<set rowId='Chocolate' columnId='Turkey Hill' value='5' displayValue='OUR PICK!!!'/>
		<set rowId='Chocolate' columnId='Carvel' showValue='0' value='4'/>
		<set rowId='Chocolate' columnId='Breyers' showValue='0' value='3'/>

		<set rowId='Vanilla' columnId='Baskin-Robbins' showValue='0' value='3'/>
		<set rowId='Vanilla' columnId='Blue Bunny' showValue='0' value='3'/>
		<set rowId='Vanilla' columnId='Turkey Hill' showValue='0' value='2'/>
		<set rowId='Vanilla' columnId='Carvel' showValue='0' value='2'/>
		<set rowId='Vanilla' columnId='Breyers' showValue='0' value='4'/>

		<set rowId='Strawberry' columnId='Baskin-Robbins' showValue='0' value='4'/>
		<set rowId='Strawberry' columnId='Blue Bunny' showValue='0' value='3'/>
		<set rowId='Strawberry' columnId='Turkey Hill' showValue='0' value='3'/>
		<set rowId='Strawberry' columnId='Carvel' showValue='0' value='4'/>
		<set rowId='Strawberry' columnId='Breyers' showValue='0' value='4'/>

		<set rowId='Butter Pecan' columnId='Baskin-Robbins' showValue='0' value='2'/>
		<set rowId='Butter Pecan' columnId='Blue Bunny' showValue='0' value='3'/>
		<set rowId='Butter Pecan' columnId='Turkey Hill' showValue='0' value='3'/>
		<set rowId='Butter Pecan' columnId='Carvel' showValue='0' value='4'/>
		<set rowId='Butter Pecan' columnId='Breyers' showValue='0' value='3'/>

		<set rowId='Pistachio' columnId='Baskin-Robbins' showValue='0' value='3'/>
		<set rowId='Pistachio' columnId='Blue Bunny' showValue='0' value='4'/>
		<set rowId='Pistachio' columnId='Turkey Hill' showValue='0' value='3'/>
		<set rowId='Pistachio' columnId='Carvel' showValue='0' value='3'/>
		<set rowId='Pistachio' columnId='Breyers' showValue='0' value='2'/>

   </dataset>

	<colorRange gradient='0'>
        <color minValue='0' maxValue='3' code='C3D9EE' label='Nothing great'/>
        <color minValue='3' maxValue='4' code='FBF59D' label='Ok, can try'/>
        <color minValue='4' maxValue='5' code='C8DC6B' label='Yummy, must have'/> 
     </colorRange>

	<styles>
		
	    <definition>
	        <style name='DataValueStyle' type='font' color='FFFFFF' bold='1' size='12'/>
	        <style name='CaptionStyle' type='font' color='CE0355' bold='1'/>
	    </definition>

	    <application>
	        <apply toObject='DataValues' styles='DataValueStyle' />
	        <apply toObject='Caption' styles='CaptionStyle' />
	    </application>
	
	</styles>
	
</chart>

